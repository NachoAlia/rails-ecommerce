<h1>Iniciar sesión</h1>
<%=render 'sessionErrors'%>
<%= form_tag '/login' do %>
<%= label_tag :email%>
<%= text_field_tag :email %><br>
<%= label_tag :password%>
<%= password_field_tag :password%><br>
<%= submit_tag "Iniciar sesión", class:"btn btn-primary"%>
<%end%>
<%= button_to 'Volver', '/', method: :get %>
